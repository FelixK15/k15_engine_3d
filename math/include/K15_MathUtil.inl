Real MathUtil::sin(Real p_Angle)
{
	return ::sin(p_Angle);
}
/*********************************************************************************/
Real MathUtil::cos(Real p_Angle)
{
	return ::cos(p_Angle);
}
/*********************************************************************************/
Real MathUtil::tan(Real p_Angle)
{
	return ::tan(p_Angle);
}
/*********************************************************************************/
Real MathUtil::asin(Real p_Sin)
{
	return ::asin(p_Sin);
}
/*********************************************************************************/
Real MathUtil::acos(Real p_Cos)
{
	return ::acos(p_Cos);
}
/*********************************************************************************/
Real MathUtil::atan(Real p_Tan)
{
	return ::atan(p_Tan);
}
/*********************************************************************************/
Real MathUtil::atan2(Real p_X, Real p_Y)
{
	return ::atan2(p_X, p_Y);
}
