Real MathUtil::sqrt(Real p_Value)
{
	return ::sqrt(p_Value);
}
/*********************************************************************************/
Real MathUtil::pow(Real p_Base, Real p_Exponent)
{
	return ::pow(p_Base,p_Exponent);
}
/*********************************************************************************/
Real MathUtil::exp(Real p_Value)
{
	return ::exp(p_Value);
}
/*********************************************************************************/
Real MathUtil::abs(Real p_Value)
{
	return ::fabs(p_Value);
}
/*********************************************************************************/
Real MathUtil::log(Real p_Value)
{
	return ::log(p_Value);
}
/*********************************************************************************/
Real MathUtil::log10(Real p_Value)
{
	return ::log10(p_Value);
}
